  "scripts": {
    "build": "npm run build:clean && npm run build:init && npm run build:bundle && npm run build:preamble && npm run build:app",
    "build:app": "cat ./build/preamble.js ./build/bundle.js > ./lib/Code.gs && find ./src/api -type f -name '*.js' -exec sh -c \"cat {} | sed -n '/var ns =/, /\\/\\/ EXPORTS \\/\\// p' | sed '1d' | sed '/^\\/\\//d' | sed 's/\\/\\/ eslint-disable-line.*$//' | sed '/^$/d'\" \\; >> ./lib/Code.gs",
    "build:bundle": "./node_modules/.bin/esbuild ./src/index.js --bundle --minify --format=iife --legal-comments=none --global-name=ns --outfile=./build/bundle.js",
    "build:clean": "rm -rf ./build",
    "build:init": "mkdir ./build",
    "build:preamble": "echo '/** @OnlyCurrentDoc */\nvar global = {};' > ./build/preamble.js",
    "clasp:pull": "./node_modules/.bin/clasp pull",
    "clasp:push": "./node_modules/.bin/clasp push",
    "clasp:status": "./node_modules/.bin/clasp status",
    "publish": "npm run build && ((git add . && git commit -m 'Build app' && git push origin main) || echo 'Nothing to publish') && npm run clasp:push",
    "test": "make test",
    "test-cov": "make test-cov"
  },
  "homepage": "https://stdlib.io",
